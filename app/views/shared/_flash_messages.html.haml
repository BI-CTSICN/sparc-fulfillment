#flash
  - flash.each do |type, message|
    .alert.alert-dismissable{ :class => twitterized_type(type) }
      %button.close{:'data-dismiss' => "alert", :type => "button"}
        %span{:'aria-hidden' => "true"} &times;
        %span.sr-only Close
      %p= message
= javascript_include_tag 'flash_messages'