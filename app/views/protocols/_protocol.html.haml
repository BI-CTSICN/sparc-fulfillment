.fixed-table-container.protocol
  %table.protocol.details.custom_striped{id: dom_id(protocol), "data-toggle" => 'table', "data-url" => "/protocols/#{protocol.sparc_id}.json"}
    %thead
      %tr
        %th{data: {class: "sparc_id", align: "left", sortable: "false", field: "sparc_id"}}= t(:index_page)[:sparc_id]
        %th{data: {class: "pi", align: "left", sortable: "true", field: "pi"}}= t(:index_page)[:principal_investigator]
        %th{data: {class: "coordinators", align: "center", sortable: "false", field: "coordinators"}}= t(:index_page)[:coordinators]
        %th{data: {class: "short_title", align: "left", sortable: "false", field: "short_title"}}= t(:index_page)[:short_title]
        %th{data: {class: "udak_project_number", align: "left", sortable: "false", field: "udak_project_number"}}= t(:index_page)[:udak_project_number]
        %th{data: {class: "irb_expiration_date", align: "center", sortable: "false", field: "irb_expiration_date"}}= t(:index_page)[:irb_expiration_date]
