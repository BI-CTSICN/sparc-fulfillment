.col-xs-12
  .row.calendar
    .col-xs-12
      %table.table.table-bordered.table-hover
        %thead
          %tr
            %th.service
              = t(:study_level_charges)[:service]
            %th.qty_req
              = t(:study_level_charges)[:qty_req]
            %th.qty_type
              = t(:study_level_charges)[:qty_type]
            %th.qty_rem
              = t(:study_level_charges)[:qty_rem]
            %th.started
              = t(:study_level_charges)[:started]
            %th.last_fulfillment
              = t(:study_level_charges)[:last_fulfillment]
            %th.components
              = t(:study_level_charges)[:components]
            %th.notes
              = t(:study_level_charges)[:notes]
            %th.documents
              = t(:study_level_charges)[:documents]
            %th.fulfillments
              = t(:study_level_charges)[:fulfillments]
        %tbody
          -protocol.line_items.where(otf: true).each do |li|
            = render partial: "/study_level_charges/one_time_fee", locals: {line_item: li}