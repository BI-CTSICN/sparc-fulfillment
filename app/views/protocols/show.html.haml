= render :partial => 'protocol_details', locals: {protocol: @protocol}
.clear
%div{:role => 'tabpanel'}
  %ul.nav.nav-tabs{:role => 'tablist'}
    %li.active{:role => 'presentation'}= link_to "Study Schedule", "#study_schedule",{"data-toggle" => 'tab', :role => 'tab', "aria-controls" => 'study_schedule'}
    %li{:role => 'presentation'}= link_to "Participant List", "#participant_list",{"data-toggle" => 'tab', :role => 'tab', "aria-controls" => 'participant_list'}
    %li{:role => 'presentation'}= link_to "Particiant Tracker", "#participant_tracker",{"data-toggle" => 'tab', :role => 'tab', "aria-controls" => 'participant_tracker'}
    %li{:role => 'presentation'}= link_to "Study Level Charges", "#study_level_charges",{"data-toggle" => 'tab', :role => 'tab', "aria-controls" => 'study_level_charges'}
    %li{:role => 'presentation'}= link_to "Audit Reporting", "#audit_reporting",{"data-toggle" => 'tab', :role => 'tab', "aria-controls" => 'audit_reporting'}

  .tab-content
    .tab-pane.active#study_schedule{:role => 'tabpanel'}
      = render :partial => 'protocols/study_schedule/schedule_edit_buttons'
      = render :partial => 'protocols/study_schedule/calendar'

    .tab-pane#participant_list{:role => 'tabpanel'}
      = render partial: 'participant_list', locals: {protocol: @protocol}

    .tab-pane#participant_tracker{:role => 'tabpanel'}
      = render 'participant_tracker'

    .tab-pane#study_level_charges{:role => 'tabpanel'}
      = render 'study_level_charges'

    .tab-pane#audit_reporting{:role => 'tabpanel'}
      = render 'audit_reporting'
