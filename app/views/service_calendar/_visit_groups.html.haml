- visit_groups.each_with_index do |visit_group, index|
  .col-xs-1.visit_group_box
    .row.visit_group_day_windows.text-center
      .col-xs-4.text-center.day_window
        %span{id: "visit-before-display-#{visit_group.id}"}= visit_group.window_before || 0
      .col-xs-4.text-center.day_window
        %span{id: "visit-day-display-#{visit_group.id}"}= visit_group.day
      .col-xs-4.text-center.day_window
        %span{id: "visit-after-display-#{visit_group.id}"}= visit_group.window_after || 0
    .row.visit_group_names.text-center
      .visit_name_display.text-center{:data_visit_group_id => visit_group.id}
        %span{id: "visit-name-display-#{visit_group.id}"}= text_field_tag "visit_group_#{visit_group.id}", visit_group.name, class: "form-control visit_group_#{index} visit_name", 'previous_name' => visit_group.name, :'data-visit_group_id' => visit_group.id
- (visit_groups.size...Visit.per_page).each do |index|
  .col-md-1.text-center{class: "visit_group_#{index}"}
    &nbsp
    .row
      .col-md-12 &nbsp


