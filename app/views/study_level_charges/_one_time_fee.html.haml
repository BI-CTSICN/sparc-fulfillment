%tr.one_time_fee{data: {line_item: {id: line_item.id}}}
  %td.service
    = line_item.service.name
  %td.qty_req
    = line_item.quantity
  %td.qty_type
    = line_item.quantity
  %td.qty_rem
    = line_item.quantity_remaining
  %td.started
    = line_item.date_started
  %td.last_fulfillment
    = line_item.last_fulfillment
  %td.components
    = line_item.quantity
  %td.notes
    = line_item.quantity
  %td.documents
    = line_item.quantity
  %td.fulfillments
    = line_item.quantity